<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="com.example.Sample.Wizard"
    version="1.0.0"
    api-version="4"
    xmlns="http://tizen.org/ns/packages">
    <profile name="wearable" />
    <widget-application
        appid="com.example.Sample.Wizard"
        exec="Sample.Wizard.dll"
        type="dotnet"
        main="true"
        update-period="0">
        <label>Sample.Wizard</label>
        <icon>Sample.Wizard.png</icon>
        <metadata
            key="http://tizen.org/metadata/prefer_dotnet_aot"
            value="true" />
        <support-size preview="Sample.Wizard.png">2x2</support-size>
    </widget-application>
    <privileges>
        <privilege>http://tizen.org/privilege/widget.viewer</privilege>
        <privilege>http://tizen.org/privilege/haptic</privilege>
        <privilege>http://tizen.org/privilege/healthinfo</privilege>
    </privileges>
</manifest>
